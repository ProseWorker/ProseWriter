  "scripts": {
    "clean": "rimraf build",
    "copy:i18n": "mkdir -p ./build/shared/i18n && cp -R ./shared/i18n/locales ./build/shared/i18n",
    "build:i18n": "i18next --silent 'shared/**/*.tsx' 'shared/**/*.ts' 'app/**/*.tsx' 'app/**/*.ts' && yarn copy:i18n",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build:shared": "babel --quiet --extensions .ts,.tsx -d./build/shared ./shared",
    "build": "yarn clean && yarn build:webpack && yarn build:i18n && yarn build:shared",
    "start": "DEPLOYMENT=fdsfd webpack serve --config development.config.js",
    "dev:watch": "nodemon --exec \"yarn build:server && yarn dev\" -e js,ts --ignore build/ --ignore app/ --ignore shared/editor",
    "lint": "eslint app shared",
    "prepare": "husky install",
    "test": "yarn test:app",
    "test:app": "jest --config=app/.jestconfig.json --runInBand --forceExit",
    "test:watch": "jest --config=server/.jestconfig.json --runInBand --forceExit --watchAll"
  },
